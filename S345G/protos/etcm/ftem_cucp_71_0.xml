<flex xmlns="urn:com:ericsson:schema:xml:oss:ftem">
    <date>2022-10-12</date>
    <major_version>71</major_version>
    <correction_version>0</correction_version>
    <managed_function>CUCP</managed_function>
    <filters>
        <filter name="PrefUeGrpId">
            <description>Identifier of preference UE group used to determine some UE property that requires specific configuration data.</description>
            <range>
                <min>0</min>
                <max>255</max>
            </range>
        </filter>
        <filter name="UeGrpId">
            <description>Identifier of UE group used to determine UE configuration data.</description>
            <range>
                <min>0</min>
                <max>255</max>
            </range>
        </filter>
        <filter name="UePowerClass">
            <description>UE power class value.</description>
            <enumeration>
                <member name="UE_POWER_CLASS_NO_VALUE">0</member>
                <member name="UE_POWER_CLASS_1">1</member>
                <member name="UE_POWER_CLASS_1_5">2</member>
                <member name="UE_POWER_CLASS_2">3</member>
                <member name="UE_POWER_CLASS_3">4</member>
                <member name="UE_POWER_CLASS_4">5</member>
            </enumeration>
        </filter>
        <filter name="PlmnId">
            <description>Identifier of PLMN. Consists of MCC and MNC.</description>
            <custom/>
        </filter>
        <filter name="Snssai">
            <description>Single-Network Slice Selection Assistance Information composed of SST and SD.</description>
            <custom/>
        </filter>
        <filter name="Qos">
            <description>QoS value of bearers for which latency and contention delay values are measured. Can be either QCI value (NSA) or 5QI value (SA).</description>
            <range>
                <min>0</min>
                <max>255</max>
            </range>
        </filter>
        <filter name="NrDcConfigured">
            <description>Whether UE has NR-DC configured.</description>
            <enumeration>
                <member name="NRDC_CONFIGURED_NO_VALUE">0</member>
                <member name="NRDC_CONFIGURED_TRUE">1</member>
                <member name="NRDC_CONFIGURED_FALSE">2</member>
            </enumeration>
        </filter>
        <filter name="UeMobilityGrpId">
            <description>Identifier of UE mobility group used to determine UE configuration data.</description>
            <range>
                <min>0</min>
                <max>255</max>
            </range>
        </filter>
        <filter name="UeServiceGrpId">
            <description>Identifier of UE service group used to determine UE configuration data.</description>
            <range>
                <min>0</min>
                <max>255</max>
            </range>
        </filter>
        <filter name="Arp">
            <description>Allocation and Retention Priority Level value.</description>
            <range>
                <min>0</min>
                <max>15</max>
            </range>
        </filter>
        <filter name="TrafficSteeringType">
            <description>Type of traffic steering.</description>
            <enumeration>
                <member name="TRAFFIC_STEERING_TYPE_NO_VALUE">0</member>
                <member name="TRAFFIC_STEERING_TYPE_MULTI_LAYER_COORDINATION">1</member>
                <member name="TRAFFIC_STEERING_TYPE_BW_TRIGGERED_INTER_SYSTEM_HANDOVER">2</member>
                <member name="TRAFFIC_STEERING_TYPE_NOT_APPLICABLE">3</member>
            </enumeration>
        </filter>
        <filter name="LocReportType">
            <description>Type of Location Reporting.</description>
            <enumeration>
                <member name="LOC_REPORT_TYPE_NO_VALUE">0</member>
                <member name="LOC_REPORT_TYPE_DIRECT">1</member>
                <member name="LOC_REPORT_TYPE_CHANGE_CELL">2</member>
                <member name="LOC_REPORT_TYPE_AREA">3</member>
            </enumeration>
        </filter>
        <filter name="Spid">
            <description>Subscriber profile ID value.</description>
            <range>
                <min>1</min>
                <max>256</max>
            </range>
        </filter>
    </filters>
